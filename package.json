{
  "name": "tfcdk",
  "version": "1.0.0",
  "main": "main.js",
  "types": "main.ts",
  "license": "MPL-2.0",
  "private": true,
  "scripts": {
    "prebuild": "rm -rf dist ",
    "build": "tsc",
    "postbuild": "resolve-tspaths",
    "synth": "cdktf synth",
    "compile": "tsc --pretty",
    "watch": "tsc -w",
    "startFunc": "func start --script-root dist  --typescript",
    "start": "yarn startFunc  & npm run watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "upgrade": "npm i cdktf@latest cdktf-cli@latest",
    "upgrade:next": "npm i cdktf@next cdktf-cli@next",
    "startDist": "cd ./dist && func start"
  },
  "engines": {
    "node": ">=14.0"
  },
  "dependencies": {
    "@azure/functions": "^3.5.0",
    "@azure/static-web-apps-cli": "^1.0.6",
    "module-alias": "^2.2.2",
    "mongoose": "^7.0.1",
    "resolve-tspaths": "^0.8.8",
    "ts-dotenv": "^0.9.1",
    "tsconfig-paths": "^4.1.2",
    "tspath": "^2.6.8"
  },
  "devDependencies": {
    "@cdktf/provider-azurerm": "^5.0.6",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.2",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "cdktf": "^0.15.5",
    "constructs": "^10.1.262",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.4.3",
    "nodemon": "^2.0.20",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
